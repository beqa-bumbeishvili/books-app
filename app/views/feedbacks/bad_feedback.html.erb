<p id="notice"><%= notice %></p>

<h1>bad feedbacks</h1>

<table>
  <thead>
  <tr>
    <th>Comment</th>
    <th>Score</th>
    <th>Feedbacker</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.comment %></td>
        <td><%= feedback.score %></td>
        <td><%= feedback.user.name_and_lastname %></td>
        <td><%= link_to 'change_feedback_weight', change_feedback_status_path(feedback) %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feedback', new_feedback_path %>
