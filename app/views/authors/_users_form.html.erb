<h2><%= title %> </h2>
<table>
  <thead>
  <tr>
    <th>User name</th>
    <th>last name</th>
    <th>birth date</th>
    <th>pin</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% data.each do |item| %>
      <tr>
        <td><%= item.name %></td>
        <td><%= item.last_name %></td>
        <td><%= item.birth_date %></td>
        <td><%= item.pin %></td>
      </tr>
  <% end %>
  </tbody>
</table>