<h2><%= title %> </h2>
<table>
  <thead>
  <tr>
    <th>Book Number</th>
    <th>Book Title</th>
    <th>Book Published At</th>
    <th>Name</th>
    <th>Lastname</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% data.each do |item| %>
      <tr>
        <td><%= item.book_number %></td>
        <td><%= item.book_title %></td>
        <td><%= item.book_published_at %></td>
        <td><%= item.author_name %></td>
        <td><%= item.author_last_name %></td>
      </tr>
  <% end %>
  </tbody>
</table>